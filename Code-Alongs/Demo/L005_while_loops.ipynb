   ],
   "source": [
    "counter = 10\n",
    "\n",
    "while counter > 0:\n",
    "    print(f\"{counter:2d}: Hello World!\")\n",
    "    counter -= 1\n",
    "\n",
    "\n",
    "print(\"End.\")"
   ]
  }
